<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

	<!--ventasptoemision ATS-->

    	<record model="ir.ui.view" id="ats_ventasptoemision_search_view">
		<field name="name">ats_ventasptoemision.search</field>
		<field name="model">fiscal.ats_ventaptoemision</field>
		<field name="arch" type="xml">
		    <search string="Session Search">
				<field name="establecimiento" />
				
		    </search>
		</field>
	    </record>              

        <record model="ir.ui.view" id="fiscal_ats_ventasptoemision_view_form">
            <field name="name">fiscal.ats_ventaptoemision.form</field>
            <field name="model">fiscal.ats_ventaptoemision</field>
            <field name="arch" type="xml">
                <form string="Punto Emision" version="7.0" >
		   <sheet>
					<group colspan="2" col="4">
						
						
						<field name='establecimiento'  />
						<field name="total" />
					</group>
		     </sheet>
		   </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="fiscal_ats_ventasptoemision_view_tree">
            <field name="name">fiscal.ats_ventaptoemision.tree</field>
            <field name="model">fiscal.ats_ventaptoemision</field>
            <field name="arch" type="xml">
                <tree string="Punto Emision" >
	
					<field name="establecimiento" />
					<field name="total" />
					
                </tree>
            </field>
        </record>

    </data>
</openerp>
